{
	"ignorePatterns": [
		"*.json"
	],
	"rules": {
		"semi": "off",
		"indent": [
			"error",
			4
		],
		"no-tabs": [
			"off"
		],
		"quotes": [
			"error",
			"single"
		],
		"comma-dangle": "off",
		"space-before-function-paren": "off",
		"no-use-before-define": "off",
		"@typescript-eslint/no-use-before-define": [
			"error"
		]
	}
}